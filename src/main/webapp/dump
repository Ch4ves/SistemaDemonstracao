CREATE TABLE tetra (
    id int NOT NULL AUTO_INCREMENT,
    nome varchar(255) NOT NULL,
    email varchar(255) NOT NULL,
    endereco varchar(255) NOT NULL,
    telefone varchar(13) NOT NULL,
    sexo varchar(10) NOT NULL,
    newsletter varchar(15),
    PRIMARY KEY (id)
);



